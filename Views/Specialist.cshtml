@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Specialist>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "Layout.cshtml";
}

@Model.Value("specialist_Title")



